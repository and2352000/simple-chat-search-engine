  "description": "",
  "dependencies": {
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.3.11",
    "@langchain/openai": "^0.6.2",
    "dotenv": "^17.2.0",
    "langchain": "^0.3.30",
    "typeorm": "^0.3.27",
    "typescript": "^5.9.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "ts-node": "^10.9.2"
  }
